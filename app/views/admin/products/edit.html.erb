<% @page_title = '管理画面：商品編集' %>

<div class="page-header">
  <h1>商品の編集</h1>
</div>

<%= render 'shared/errors', obj: @product %>

<%= form_for [:admin, @product], class: 'form-horizontal' do |f| %>
  <%= render 'form', f: f %>
  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit '商品を更新する', class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>
